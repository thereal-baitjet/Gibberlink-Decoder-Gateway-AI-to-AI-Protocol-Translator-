{
  "name": "@gibberlink/node-client",
  "version": "1.0.0",
  "description": "Node.js client example for Gibberlink Gateway",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "node dist/test.js",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@gibberlink/protocol-core": "workspace:*",
    "@gibberlink/transports": "workspace:*",
