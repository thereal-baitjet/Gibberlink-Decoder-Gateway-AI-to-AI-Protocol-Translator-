    "ws": "^8.14.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "eslint": "^8.55.0"
  },
  "keywords": [
    "gibberlink",
    "node",
    "client",
    "example"
